#!/usr/bin/env bash
# This script displays the names of files in the current directory, removing any prefix up to the first dash.

for file in *; do
  if [[ -e "$file" ]]; then  # Check if the file exists
    if [[ "$file" == *-* ]]; then  # Check if the file name contains a dash
      echo "${file#*-}"  # Remove everything up to and including the first dash
    else
      echo "$file"  # Print the file name as it is
    fi
  fi
done
